{
  "name": "contentful/contentful-management",
  "description": "SDK for the Contentful Content Management API",
  "type": "library",
  "license": "MIT",
  "require": {
    "php": "^7.0",
    "contentful/core": "^1.0@beta",
    "symfony/console": "^3.0|^4.0",
    "symfony/filesystem": "^3.0|^4.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^6.2",
    "php-vcr/phpunit-testlistener-vcr": "^3.0",
    "php-vcr/php-vcr": "1.3.1",
    "nikic/php-parser": "^3.1"
  },
  "autoload": {
    "psr-4": {
      "Contentful\\Management\\": "src/"
    }
  },
  "autoload-dev": {
      "psr-4": {
        "Contentful\\Tests\\Management\\": "tests/"
      }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0.0-dev"
    }
  },
  "scripts": {
      "test": "vendor/bin/phpunit"
  },
  "suggest": {
    "nikic/php-parser": "Allows generating content type classes"
  }
}
